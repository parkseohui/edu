	<script type="text/javascript">
	$(function() {
		
		//관리비 부과기초작업 페이지 이동
		$("#levyJob").on("click", function() {
			self.location = "/keeper/levyRegister";
		});
		
		
	});

	

	
		function admitUser(num) {
			console.log(num);
			$.ajax({
				url: "/keeper/admitUser",
				method: "POST",
				dataType: "json",
				contentType: "application/json",
				data: JSON.stringify({
					"seq": num
				}),
				success: function(data) {
					console.log(data);
					if(data.status == "true"){
						alert("승인에 성공하셨습니다.");
						location.reload();
					}else {
						alert("조금 있다 다시 시도해주세요.");
					}
				}
			})
		}
